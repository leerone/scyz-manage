<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.lensyn.gxpt.service.mapper.CasesMapper">
	<resultMap id="BaseResultMap" type="com.lensyn.gxpt.service.entity.Cases">
		<id column="id" jdbcType="INTEGER" property="id" />
		<result column="name" jdbcType="VARCHAR" property="name" />
		<result column="pic" jdbcType="VARCHAR" property="pic" />
		<result column="url" jdbcType="VARCHAR" property="url" />
		<result column="type" jdbcType="VARCHAR" property="type" />
		<result column="address" jdbcType="VARCHAR" property="address" />
		<result column="content" jdbcType="VARCHAR" property="content" />
		<result column="location" jdbcType="VARCHAR" property="location" />
		<result column="coverage" jdbcType="VARCHAR" property="coverage" />
		<result column="height" jdbcType="VARCHAR" property="height" />
		<result column="finishtime" jdbcType="VARCHAR" property="finishtime" />
		<result column="scale" jdbcType="VARCHAR" property="scale" />
	</resultMap>
	
	<select id="getCasesList" resultMap="BaseResultMap">
		select * from cases
	</select>

	<insert id="insertNews" parameterType="com.lensyn.gxpt.service.entity.Cases">
		insert into cases (name,
		pic, url, type, address, content, location,coverage,height,finishtime,scale) VALUES (#{name}, #{pic}, #{url}, #{type},
		#{
		address}, #{content}, #{location}, #{coverage}, #{height}, #{finishtime}, #{scale})
	</insert>
	
	<select id="delCases" parameterType="java.lang.String">
		delete from cases where id = #{id}
	</select>
	
</mapper>